            "resource": [
                {
                    "@type": "dctypes:Text",
                    "chars": "<p>小幡千春、中村</p>",
                    "format": "text/html"
                },
                {
                    "@type": "oa:Tag",
                    "chars": "函館東濱町廿、廿一番地"
                },
                {
                    "@type": "oa:Tag",
                    "chars": "宮路店"
                },
                {
                    "@type": "oa:Tag",
                    "chars": "拝啓大暑の節御座候處盆御機嫌克奉大賀候毎々御引立を蒙り不淺難有奉存候然るに家屋手狭の為混雑致し自然不行届の儀有之何共奉恐入候依て今回隣地を合併致し従来の如く回漕及旅人宿営業仕候間一層の御愛顧を奉希候先は暑中御伺旁御聴申上度短書候頓首"
                },
                {
                    "@type": "oa:Tag",
                    "chars": "明治16年7月20日"
                },
                {
                    "@type": "oa:Tag",
                    "chars": "追啓本文の儀御懇意の御方々様へ御吹聴御引立被下度奉願候不具"
                }
            ]
        },
